{
    "diagnostic.errorSign": "✖",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "ℹ",
    "diagnostic.hintSign": "➤",
    "coc.preferences.extensionUpdateCheck": "daily",
    "coc.preferences.formatOnSaveFiletypes": [
        "kt",
        "yaml",
        "java",
        "python"
    ],
    "suggest.noselect": false,
    "codeLens.enable": true,
    "signature.enable": true,
    "suggest.preferCompleteThanJumpPlaceholder": true,
    "tsserver.implicitProjectConfig.experimentalDecorators": true,
    "pyright.disableDiagnostics": true,
    "python.formatting.blackArgs": ["--fast"],
    "python.sortImports.args": ["--profile", "black"],
    "python.formatting.provider": "black",
    "python.formatting.blackPath": "~/.virtualenvs/neovim3.9/bin/black",
    "python.sortImports.path": "~/.virtualenvs/neovim3.9/bin/isort",
    "python.linting.pylintPath": "~/.virtualenvs/neovim3.9/bin/pylint",
    "python.linting.pylintArgs": ["--load-plugins", "pylint_django"],
    "python.linting.pylintEnabled": true,
    "python.linting.mypyPath": "~/.virtualenvs/neovim3.9/bin/mypy",
    "python.linting.mypyEnabled": true,
    "python.venvPath": "~/.virtualenvs",
    "html.format.enable": true,
    "html.validate.scripts": true,
    "html.validate.styles": true,
    "html.autoClosingTags": true,
    "svelte.enable-ts-plugin": true,
    "java.home": "~/.jenv/versions/cocjavahome",
    "java.referencesCodeLens.enabled": true,
    "java.implementationsCodeLens.enabled": true,
    "java.trace.server": "verbose",
    "java.maxConcurrentBuilds": 1,
    "java.completion.enabled": true,
    "javascript.format.enabled": true,
    "java.import.maven.enabled": true,
    "java.import.gradle.enabled": true,
    "java.format.enabled": true,
    "java.autobuild.enabled": true,
    "java.completion.guessMethodArguments": true,
    "java.codeGeneration.generateComments": true,
    "java.project.resourceFilters": ["node_modules", ".git", "*.class"]
}
