{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "google_auth": false,
  "sisyphus_agent": {
    "disabled": false,
    "planner_enabled": true,
    "replace_plan": true
  },
  "agents": {
    "sisyphus": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.3
    },
    "prometheus": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.1
    },
    "metis": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.2
    },
    "momus": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.3
    },
    "atlas": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.2
    },
    "oracle": {
      "model": "openai/gpt-5.2-codex",
      "temperature": 0.5
    },
    "explore": {
      "model": "github-copilot/gpt-5-mini",
      "temperature": 0.1
    },
    "librarian": {
      "model": "opencode/big-pickle",
      "temperature": 0.1
    },
    "multimodal-looker": {
      "model": "github-copilot/gemini-3-flash-preview"
    }
  },
  "categories": {
    "ultrabrain": {
      "model": "openai/gpt-5.2-codex",
      "temperature": 0.1
    },
    "business-logic": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.1
    },
    "visual-engineering": {
      "model": "github-copilot/claude-sonnet-4.5",
      "temperature": 0.7
    },
    "artistry": {
      "model": "github-copilot/claude-opus-4.5",
      "temperature": 0.9
    },
    "general": {
      "model": "github-copilot/claude-sonnet-4.5"
    },
    "unspecified-high": {
      "model": "github-copilot/claude-sonnet-4.5"
    },
    "quick": {
      "model": "github-copilot/gpt-5-mini"
    },
    "writing": {
      "model": "github-copilot/gpt-5-mini"
    },
    "unspecified-low": {
      "model": "github-copilot/gpt-5-mini"
    }
  },
  "background_task": {
    "defaultConcurrency": 5,
    "providerConcurrency": {
      "github-copilot": 10,
      "opencode": 5,
      "openai": 3
    }
  },
  "disabled_hooks": [
    "startup-toast"
  ],
  "disabled_skills": []
}
